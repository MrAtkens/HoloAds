{
  "name": "hologram",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "emailjs-com": "^2.6.4",
    "framer-motion": "^4.1.5",
    "react": "^17.0.2",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-scroll-up": "^1.3.7",
    "react-slick": "^0.28.1",
    "react-yandex-maps": "^4.6.0",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.3.0",
    "tailwindcss": "^2.1.4",
    "twin.macro": "^2.4.2",
    "web-vitals": "^2.0.1"
